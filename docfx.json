{
  "metadata": [
    {
      "src": [
        {
          "files": ["Mustaverse.DEAL.Runtime.csproj"],
          "src": ".."
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "filter": "filterConfig.yml"
    }
  ],
  "build": {
    "globalMetadata": {
      "_appLogoPath": "images/Logo.png",
      "_enableSearch": "true",
      "pdf": true
    },
    "content": [
      {
        "files": ["api/**.yml", "api/index.md"]
      },
      {
        "files": ["articles/**.md", "articles/**/toc.yml", "toc.yml", "*.md"]
      }
    ],
    "resource": [
      {
        "files": ["images/**"]
      }
    ],
    "overwrite": [
      {
        "files": ["apidoc/**.md"],
        "exclude": ["obj/**", "docs/**"]
      }
    ],
    "dest": "docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "disableGitFeatures": false
  },
  "pdf": {
    "content": [
      {
        "files": ["PDF/toc.yml"]
      },
      {
        "files": ["reference/**.yml", "reference/index.md"],
        "exclude": ["**/toc.yml", "**/toc.md"]
      },
      {
        "files": ["Concepts/**.md", "Concepts/**/toc.yml", "toc.yml", "*.md"],
        "exclude": [
          "**/bin/**",
          "**/obj/**",
          "PDF/**",
          "**/toc.yml",
          "**/toc.md"
        ]
      }
    ],
    "resource": [
      {
        "files": ["images/**"],
        "exclude": ["**/bin/**", "**/obj/**", "PDF/**"]
      }
    ],
    "dest": "_pdf",
    "outline": "NoOutline"
  }
}
